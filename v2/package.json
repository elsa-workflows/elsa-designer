{
  "name": "elsa-workflows",
  "version": "2.0.0",
  "description": "Elsa Designer is a standalone client-side HTML5 web component that allows you to visually design Elsa workflows that are stored in Elsa Workflow servers",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/elsa-workflows/elsa-workflows.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "latest",
    "@panzoom/panzoom": "latest",
    "@stencil/core": "latest",
    "@stencil/sass": "latest",
    "bootstrap-scss": "latest",
    "bs-components": "https://www.myget.org/F/bs-components/npm/bs-components/-/0.1.9.tgz",
    "graphql-request": "latest",
    "inversify": "latest",
    "jsplumb": "latest",
    "monaco-editor": "latest",
    "reflect-metadata": "latest",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "uuid-browser": "^3.1.0"
  },
  "dependencies": {},
  "license": "MIT"
}
